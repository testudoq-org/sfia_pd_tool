{
    "name": "sfia",
    "version": "1.0.0",
    "description": "Welcome to the SFIA POSITION DESCRIPTION Tool, an innovative generator inspired by the SFIA Framework. This tool simplifies the PD creation process, allowing you to select specific skills and proficiency levels effortlessly through user-friendly check-boxes. Explore the tool [online](https://xanpho.x10.bz/index.html#/) to experience its capabilities.",
    "main": "functions-min.js",
    "directories": {
        "test": "test"
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "codeceptjs": "codeceptjs run --steps",
        "codeceptjs:headless": "HEADLESS=true codeceptjs run --steps",
        "codeceptjs:ui": "codecept-ui --app",
        "codeceptjs:demo": "codeceptjs run --steps -c node_modules/@codeceptjs/examples",
        "codeceptjs:demo:headless": "HEADLESS=true codeceptjs run --steps -c node_modules/@codeceptjs/examples",
        "codeceptjs:demo:ui": "codecept-ui --app  -c node_modules/@codeceptjs/examples",
        "start-server": "echo y | npx live-server --port=5500 --open=src/sfiapdgen.html",
        "stop-server": "taskkill /IM node.exe /F"
    },
